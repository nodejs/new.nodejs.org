{
  "ci": {
    "collect": {
      "startServerCommand": "npm run start",
      "startServerReadyPattern": "listen",
      "startServerReadyTimeout": 100000,
      "staticDistDir": "pages/en",
      "url": ["http://localhost:3000/"],
      "numberOfRuns": 5,
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "preset": "lighthouse:recommended"
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "server": {}
  }
}
