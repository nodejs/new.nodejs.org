<!DOCTYPE html>
<html lang="{{site.locale}}">
{{> html-head }}

<body>
  {{> header }}

  <div id="main">
    <div class="container">

      <h1>{{ title }}</h1>
      <ul>
        {{#each collections.tscMinutes}}
          <li>
            <a href="/{{../site.locale}}/{{ path }}/">{{ title }}</a>
          </li>
        {{/each}}
      </ul>

    </div>
  </div>

  {{> footer }}
</body>
</html>
